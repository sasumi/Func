{
	"name": "lfphp/func",
	"description": "A lite function pack for PHP.",
	"keywords": [
		"function"
	],
	"homepage": "http://blog.yikepi.com/",
	"type": "library",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Sasumi.H",
			"email": "cnsasumi@outlook.com",
			"homepage": "http://blog.yikepi.com/"
		}
	],
	"support": {
		"issues": "https://github.com/sasumi/Func/issues"
	},
	"require": {
		"php": ">=7.1",
		"ext-mbstring": "*",
		"ext-json": "*",
		"ext-curl": "*",
		"ext-pdo": "*"
	},
	"require-dev": {
		"ext-ssh2": "*",
		"jasny/phpdoc-parser": "^1.0"
	},
	"suggested": {
		"ext-openssl": "*"
	},
	"autoload": {
		"files": [
			"src/array.php",
			"src/color.php",
			"src/cron.php",
			"src/csp.php",
			"src/curl.php",
			"src/db.php",
			"src/env.php",
			"src/event.php",
			"src/file.php",
			"src/font.php",
			"src/html.php",
			"src/http.php",
			"src/image.php",
			"src/mail.php",
			"src/session.php",
			"src/sheet.php",
			"src/string.php",
			"src/time.php",
			"src/util.php"
		]
	},
	"autoload-dev": {
		"files": [
			"src/array.php",
			"src/color.php",
			"src/cron.php",
			"src/csp.php",
			"src/curl.php",
			"src/db.php",
			"src/env.php",
			"src/file.php",
			"src/font.php",
			"src/html.php",
			"src/http.php",
			"src/image.php",
			"src/session.php",
			"src/sheet.php",
			"src/string.php",
			"src/time.php",
			"src/util.php"
		],
		"psr-4": {
			"LFPhp\\Func\\TestCase\\": "test"
		}
	},
	"repositories": {
		"packagist": {
			"type": "composer",
			"url": "https://mirrors.aliyun.com/composer/"
		}
	}
}
