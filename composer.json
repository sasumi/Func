{
  "name": "lfphp/func",
  "description": "A lite function pack for PHP.",
  "keywords": [
	"function"
  ],
  "homepage": "http://blog.yikepi.com/",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
	{
	  "name": "Sasumi.H",
	  "email": "cnsasumi@outlook.com",
	  "homepage": "http://blog.yikepi.com/"
	}
  ],
  "support": {
	"issues": "https://github.com/sasumi/Func/issues"
  },
  "require": {
	"php": ">=5.6",
	"ext-mbstring": "*",
	"lfphp/logger": "*",
	"ext-json": "*",
	"ext-curl": "*",
	"ext-pdo": "*"
  },
  "require-dev": {
	"phpunit/phpunit": "8.5.8"
  },
  "autoload": {
	"files": [
	  "src/array.php",
	  "src/env.php",
	  "src/file.php",
	  "src/util.php",
	  "src/string.php",
	  "src/time.php",
	  "src/cron.php",
	  "src/db.php",
	  "src/curl.php"
	]
  },
  "autoload-dev": {
	"psr-4": {
	  "LFPhp\\Func\\TestCase\\": "test"
	}
  },
  "repositories": {
	"packagist": {
	  "type": "composer",
	  "url": "https://mirrors.aliyun.com/composer/"
	}
  }
}